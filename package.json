{
    "name": "liora",
    "version": "9.1.3",
    "description": "Liora - Advanced WhatsApp Bot",
    "type": "module",
    "license": "Apache-2.0",
    "author": {
        "name": "Naruya Izumi",
        "email": "sexystyle088@gmail.com",
        "url": "https://github.com/naruyaizumi"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/naruyaizumi/liora.git"
    },
    "homepage": "https://github.com/naruyaizumi/liora#readme",
    "bugs": {
        "url": "https://github.com/naruyaizumi/liora/issues"
    },
    "engines": {
        "bun": ">=1.3.0",
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "start": "node src/main.js",
        "dev": "node --watch src/main.js",
        "test": "echo \"No tests specified\" && exit 0",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "heroku-prebuild": "echo \"Installing dependencies...\"",
        "heroku-postbuild": "echo \"Build completed successfully!\"",
        "heroku-cleanup": "rm -rf node_modules/.cache",
        "local": "NODE_ENV=development node src/main.js",
        "production": "NODE_ENV=production node src/main.js"
    },
    "dependencies": {
        "@adiwajshing/baileys": "^6.4.0",
        "async-mutex": "^0.5.0",
        "baileys": "github:naruyaizumi/baileys",
        "eventemitter3": "^5.0.1",
        "file-type": "^21.3.0",
        "node-webpmux": "^3.2.1",
        "p-queue": "^9.1.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "winston": "^3.10.0",
        "axios": "^1.6.0",
        "mongodb": "^6.3.0",
        "redis": "^4.6.10",
        "fs-extra": "^11.1.1",
        "path": "^0.12.7",
        "os": "^0.1.2",
        "cluster": "^0.7.7",
        "pm2": "^5.3.0",
        "qrcode-terminal": "^0.12.0"
    },
    "devDependencies": {
        "prettier": "^3.8.0",
        "nodemon": "^3.0.1",
        "@types/node": "^20.8.0",
        "typescript": "^5.2.2",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0"
    },
    "imports": {
        "#api/*": "./src/lib/api/*",
        "#auth": "./src/lib/core/auth/auth.js",
        "#core/*": "./src/lib/core/*",
        "#lib/*": "./src/lib/*"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf",
        "proseWrap": "preserve",
        "htmlWhitespaceSensitivity": "css",
        "embeddedLanguageFormatting": "auto"
    },
    "trustedDependencies": [
        "baileys",
        "protobufjs",
        "sharp",
        "pm2",
        "mongodb",
        "redis"
    ],
    "keywords": [
        "whatsapp",
        "bot",
        "api",
        "messaging",
        "automation",
        "heroku",
        "nodejs"
    ],
    "private": true,
    "main": "src/main.js",
    "files": [
        "src",
        "LICENSE",
        "README.md",
        "CHANGELOG.md"
    ],
    "volta": {
        "node": "18.17.1"
    },
    "cacheDirectories": [
        "node_modules",
        ".npm"
    ],
    "resolutions": {
        "sharp": "0.33.2"
    },
    "publishConfig": {
        "access": "public"
    }
    }
